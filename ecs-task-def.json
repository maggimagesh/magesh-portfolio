{
    "family": "magesh-portfolio-task",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::983050536265:role/ecsTaskExecutionRole-magesh-portfolio",
    "containerDefinitions": [
      {
        "name": "magesh-portfolio-container",
        "image": "983050536265.dkr.ecr.ap-south-1.amazonaws.com/magesh-portfolio:latest",
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "tcp"
          }
        ],
        "essential": true,
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/magesh-portfolio",
            "awslogs-region": "ap-south-1",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "1024",
    "memory": "3072"
  }
  